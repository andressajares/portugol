algoritmo "semnome"

var

num1 : inteiro
num2 : inteiro

inicio

escreval("Digite um número: ")
leia(num1)
escreval("Digite outro número: ")
leia(num2)

se (num1 > num2)entao
escreval(num1, " é maior que ", num2)
senao
escreval(num2, " é maior que ", num1)

fimse

se (num1 = num2)entao
escreval(num1, " é identico a ", num2)

fimse

fimalgoritmo
